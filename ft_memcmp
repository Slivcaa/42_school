#include "libft.h"

int ft_memcmp(const void *s1, const void *s2, size_t n) {
    const unsigned char *ptr1 = (const unsigned char *)s1;
    const unsigned char *ptr2 = (const unsigned char *)s2;
    for (size_t i = 0; i < n; i++) {
        if (ptr1[i] != ptr2[i]) {
            return ptr1[i] - ptr2[i]; // Возвращает разницу первых разных байтов
        }
    }
    return 0; // Если все байты равны
}
int main()
{
char data1[] = "hello";
char data2[] = "hello";
char data3[] = "hellp";
ft_memcmp(data1, data2, 5); // Вернет 0, т.к. массивы равны
ft_memcmp(data1, data3, 5); // Вернет отрицательное число, т.к. 'o' < 'p'
return(0);
}
