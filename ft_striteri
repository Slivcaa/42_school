#include "libft.h"
// Реализация функции ft_striteri
void ft_striteri(char *s, void (*f)(unsigned int, char*)) {
    if (s == NULL || f == NULL) {
        return;
    }

    // Применяем функцию f к каждому символу строки s
    for (unsigned int i = 0; s[i] != '\0'; i++) {
        f(i, &s[i]);
    }
}
