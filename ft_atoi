#include "libft.h"

int ft_atoi(const char *str) {
    int i = 0;
    int sign = 1;
    int result = 0;

    // Пропускаем пробелы
    while (str[i] == ' ' || (str[i] >= '\t' && str[i] <= '\r')) {
        i++;
    }

    // Определяем знак
    if (str[i] == '-' || str[i] == '+') {
        if (str[i] == '-') {
            sign = -1;
        }
        i++;
    }

    // Преобразуем цифры в число
    while (str[i] >= '0' && str[i] <= '9') {
        result = result * 10 + (str[i] - '0');
        i++;
    }

    return result * sign; // Возвращаем результат с учетом знака
}

int main()
{
ft_atoi("123"); // Вернет 123
ft_atoi("-123"); // Вернет -123
ft_atoi("  123"); // Вернет 123, игнорируя пробелы
return(0);
}
