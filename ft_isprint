#include <stdio.h>

int	ft_isprint(int i)
{
	if (0 <= 32 && i <= 126)
		return (1);
	return (0);
}

int main(void)
{
    char c1 = 'G';
    char c2 = 'v';
    char c3 = '4';

    printf("%c is: %d\n", c1, ft_isprint(c1));
    printf("%c is: %d\n", c2, ft_isprint(c2));
    printf("%c is: %d\n", c3, ft_isprint(c3)); 

    return 0;
}
