
#include <stdio.h>

int	fft_isalnum(int i)
{
	if (('a' <= i && i <= 'z' ) || ('A' <= i && i <= 'Z')|| ('0' <= i && i <= '9'))
		return (1);
	return (0);
}

int main(void)
{
    char c1 = 'G';
    char c2 = 'k';
    char c3 = '4';

    printf("%c is: %d\n", c1, ft_isalpha(c1));
    printf("%c is: %d\n", c2, ft_isalpha(c2));
    printf("%c is: %d\n", c3, ft_isalpha(c3)); 

    return 0;
}
