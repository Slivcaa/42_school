#include <stdio.h>

int	ft_isdigit(int i)
{
	if ('0' <= i && i <= '9')
		return (1);
	return (0);
}

int main(void)
{
    char c1 = 'G';
    char c2 = 'k';
    char c3 = '4';

    printf("%c is: %d\n", c1, ft_isdigit(c1));
    printf("%c is: %d\n", c2, ft_isdigit(c2));
    printf("%c is: %d\n", c3, ft_isdigit(c3)); 

    return 0;
}
