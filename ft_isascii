#include <stdio.h>

int	ft_isascii(int i)
{
	if (0 <= i && i <= 127)
		return (1);
	return (0);
}

int main(void)
{
    char c1 = 'G';
    char c2 = 'k';
    char c3 = '4';

    printf("%c is: %d\n", c1, ft_isascii(c1));
    printf("%c is: %d\n", c2, ft_isascii(c2));
    printf("%c is: %d\n", c3, ft_isascii(c3)); 

    return 0;
}
