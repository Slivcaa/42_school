#include "libft.h"

int ft_strncmp(const char *s1, const char *s2, size_t n) 
{
    // Пока есть символы для сравнения и строки равны
    while (n > 0 && *s1 && (*s1 == *s2)) {
        s1++;  // Перейти к следующему символу в s1
        s2++;  // Перейти к следующему символу в s2
        n--;   // Уменьшить счетчик символов
    }
    // Если дошли до конца сравнения или строки одинаковы до n символов
    if (n == 0) {
        return 0;
    }
    // Возвращает разницу между символами, где строки отличаются
    return (*(unsigned char *)s1 - *(unsigned char *)s2);
}

int main()
{
ft_strncmp("hello", "helLO", 3); // Вернет 0, т.к. первые 3 символа одинаковы
ft_strncmp("hello", "helLO", 5); // Вернет положительное число, т.к. 'l' > 'L'
return(0);
}
